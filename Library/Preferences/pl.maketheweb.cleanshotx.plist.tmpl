<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Launch at login -->
	<key>LaunchAtLogin</key>
	<true/>
	<key>LaunchAtLogin__hasMigrated</key>
	<true/>

	<!-- Updates -->
	<key>SUAutomaticallyUpdate</key>
	<true/>
	<key>SUEnableAutomaticChecks</key>
	<true/>

	<!-- Privacy -->
	<key>analyticsAllowed</key>
	<false/>

	<!-- Capture settings -->
	<key>captureWithoutDesktopIcons</key>
	<true/>

	<!-- Popup behavior -->
	<key>popupAskForDestinationWhenSaving</key>
	<false/>
	<key>deletePopupAfterDragging</key>
	<true/>
	<key>popupSize</key>
	<integer>2</integer>

	<!-- Onboarding -->
	<key>onboardingDisplayed</key>
	<true/>
	<key>onboardingScrollingCaptureDisplayed</key>
	<true/>
	<key>lastAcceptedEulaVersion</key>
	<integer>1</integer>

	<!-- Hide tooltips (QTIP) -->
	<key>QTIP_0</key>
	<true/>
	<key>QTIP_2</key>
	<true/>
	<key>QTIP_4</key>
	<true/>
	<key>QTIP_7</key>
	<true/>
	<key>QTIP_9</key>
	<true/>
	<key>QTIP_13</key>
	<true/>
	<key>QTIP_16</key>
	<true/>
	<key>QTIP_17</key>
	<true/>

	<!-- Scrolling capture guide -->
	<key>shouldShowHorizontalScrollingCaptureGuide</key>
	<false/>

	<!-- Window background -->
	<key>transparentWindowBackground</key>
	<false/>

	<!-- Migration flags -->
	<key>MGR001</key>
	<true/>
	<key>MGR002</key>
	<true/>
	<key>MGR003</key>
	<true/>
	<key>MGR004</key>
	<true/>

	<!-- Annotation defaults -->
	<key>annotateLastArrowStyle</key>
	<integer>0</integer>
	<key>annotateLastHighlightShape</key>
	<integer>1</integer>
	<key>annotateLastPixelateStyle</key>
	<integer>0</integer>
	<key>annotateLastTextSize</key>
	<real>30</real>
	<key>annotateTextStyle</key>
	<integer>3</integer>
	<key>markupLastToolIndex</key>
	<integer>5</integer>
	<key>markupLastWidth</key>
	<real>7</real>
</dict>
</plist>
